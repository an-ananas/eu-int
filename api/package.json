{
  "name": "euint-api",
  "version": "1.0.0",
  "description": "REST API for scraped EU News",
  "main": "src/index.js",
  "scripts": {
    "start": "npx ts-node-dev --respawn --transpile-only ./src/index.ts",
    "test": "npx tslint -c tslint.json './src/**/*.ts'",
    "build": "npm run clean && npm run copy-files && tsc",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/"
  },
  "author": "H.C.Graamans",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "express-socket.io-session": "^1.3.5",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "socket.io": "^4.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/express-socket.io-session": "^1.3.6",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.11.6",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
